@model IEnumerable<DimaMaster.Models.CarService>

@{
    ViewBag.Title = "Index";
}

<h1>Станції технічного обслуговування</h1>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Place.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ShiftStart)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ShiftEnd)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Place.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ShiftStart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ShiftEnd)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CarServiceId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CarServiceId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CarServiceId })
        </td>
    </tr>
}

</table>
